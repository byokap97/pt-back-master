{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "@api/(.*)": "<rootDir>/../libs/api/src/$1",
        "@api": "<rootDir>/../libs/api/src"
    }
}
